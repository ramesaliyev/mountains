<div class="wrapper form-standalone">
  <div class="form">
    <h3>Registration</h3>
    <input ng-model="model.username" placeholder="User Name" type="text" />
    <input ng-model="model.password" placeholder="Password" type="password" />
    <a ng-click="register()" class="button button-primary">Register</a>
  </div>
</div>